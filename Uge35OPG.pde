void setup(){
  size(400,600);
}
void draw(){
  // Set the background
  background(225);
  
  // Make head and face
  fill(255);
  circle(200,370,300);
 
 square(130,300,50);
 square(230,300,50);
 circle(200,370,100);
 arc(200, 430, 150, 100, 0,PI,CLOSE);
  
  // Make the hat
  fill(000);
  square(100,50,200);
  
  line(50,250,350,250);
  strokeWeight(5);
  
  
  
  
}
